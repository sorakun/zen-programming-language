				</Compiler>
				<Linker>
					<Add option="-ldl" />
					<Add library="sfl/libsfl-linux.a" />
				</Linker>
			</Target>
			<Target title="DLL">
				<Option output="../bin/Zen" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add directory="../../../libraries/SFML-1.6/CSFML/include" />
				</Compiler>
				<Linker>
					<Add option="-ldl" />
					<Add library="sfl/libsfl-linux.a" />
				</Linker>
			</Target>
		</Build>
